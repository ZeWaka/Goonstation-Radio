
Raw:
$("#goonRadio").remove();
$("<div id='goonRadio' style='position: fixed; left: 0; top: 0; right: 80px; height: 40px; background: white; z-index: 99999999;'><iframe src='about:blank' style='width: 100%; height: 100%; border: none;'></iframe></div>").insertBefore("#messages");
$("#goonRadio iframe").attr("src", "https://zewaka.github.io/Goonstation-Radio/index.html");

